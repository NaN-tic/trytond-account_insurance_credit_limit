<?xml version="1.0"?>

<tryton>
  <data>

    <record model="ir.ui.view" id="partyriskanalysis_list_view">
        <field name="model">party.risk.analysis</field>
        <field name="type">tree</field>
        <field name="name">partyriskanalysis_list</field>
    </record>

    <record model="ir.ui.view" id="partyriskanalysis_form_view">
        <field name="model">party.risk.analysis</field>
        <field name="type">form</field>
        <field name="name">partyriskanalysis_form</field>
    </record>

    <record model="ir.action.act_window" id="act_partyriskanalysis">
        <field name="name">Party Risk Analysis</field>
        <field name="res_model">party.risk.analysis</field>
    </record>

    <record model="ir.action.act_window.view" id="act_partyriskanalysis_view">
      <field name="sequence" eval="20"/>
      <field name="view" ref="partyriskanalysis_form_view"/>
      <field name="act_window" ref="act_partyriskanalysis"/>
    </record>

    <record model="ir.action.act_window.view" id="act_partyriskanalysis_view1">
        <field name="sequence" eval="10"/>
        <field name="view" ref="partyriskanalysis_list_view"/>
        <field name="act_window" ref="act_partyriskanalysis"/>
    </record>

    <menuitem id="menu_party_risk_analysis" parent="menu_party_credit_root"
      sequence="30" action="act_partyriskanalysis"/>

    <!-- Wizard PartyRiskAnalysisCalculate (party.risk.analysis.calculate) -->
    <record model="ir.action.wizard" id="act_party_risk_analysis_calculate">
        <field name="name">Party Risk Analysis Calculate</field>
        <field name="wiz_name">party.risk.analysis.calculate</field>
    </record>

    <record model="ir.ui.view"
        id="party_risk_calculate_view_form">
        <field name="model">party.risk.analysis.calculate.start</field>
        <field name="type">form</field>
        <field name="name">party_risk_analysis_calculate_start_form</field>
    </record>

    <menuitem id="menu_party_risk_analysis_calculate"
        parent="menu_party_credit_root" sequence="20"
        action="act_party_risk_analysis_calculate"/>
    <!-- Wizard PartyRiskAnalysisCalculate -->
  </data>
</tryton>
